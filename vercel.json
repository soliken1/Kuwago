{
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "x-pandadoc-signature",
          "value": "$x_pandadoc_signature"
        },
        {
          "key": "Access-Control-Expose-Headers",
          "value": "x-pandadoc-signature"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/stateDocWebhook",
      "destination": "/api/stateDocWebhook.ts",
      "headers": {
        "x-pandadoc-signature": "$x_pandadoc_signature"
      }
    }
  ]
}
